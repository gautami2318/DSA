#Finding All Occurrences of an Element
def linear_search_all(arr, target):
    indices = []
    for i in range(len(arr)):
        if arr[i] == target:
            indices.append(i)  
    return indices
arr = [1, 3, 5, 3, 7, 3]
target = 3
indices = linear_search_all(arr, target)

if indices:
    print(f"Target {target} found at indices {indices}")
else:
    print(f"Target {target} not found")
